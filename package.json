{
  "name": "synology-drive-ignore-nm",
  "version": "1.1.0",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "env:init": "cp -n .env.example .env",
    "build": "bun run src/build.ts && bun run cleanup",
    "build:arm64": "bun run src/build.ts arm64 && bun run cleanup",
    "build:x64": "bun run src/build.ts x64 && bun run cleanup",
    "cleanup": "rm -f ./.*-*.bun-build"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@loice5/dangerous-ini": "^1.0.1",
    "@types/ini": "^4.1.0"
  }
}